<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated workflowspec -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="workflowspec.presentation.WorkflowspecModelWizardID"
            name="%_UI_WorkflowspecModelWizard_label"
            class="workflowspec.presentation.WorkflowspecModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/WorkflowspecModelFile.gif">
         <description>%_UI_WorkflowspecModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated workflowspec -->
      <editor
            id="workflowspec.presentation.WorkflowspecEditorID"
            name="%_UI_WorkflowspecEditor_label"
            icon="icons/full/obj16/WorkflowspecModelFile.gif"
            extensions="workflowspec"
            class="workflowspec.presentation.WorkflowspecEditor"
            contributorClass="workflowspec.presentation.WorkflowspecActionBarContributor">
      </editor>
   </extension>

</plugin>
