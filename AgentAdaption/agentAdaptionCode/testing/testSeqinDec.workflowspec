WorkflowStructure {
	workflowspec Decision {
		options {
			GuardedWorkflow {
				body Sequence {
					subworkflows {
						Task {
							ID "Undock"
							},
							Task {
								ID "Vacuum"
							}
						}
					}
				guard Atom {
					measureID "needsVacuuming"
				}
			},
			GuardedWorkflow {
				body Task {
					ID "Skip"
					}
				guard Atom {
					measureID "not needsVacuuming"
				}	
			}
		}
	}
}