WorkflowStructure {
	workflowspec Loop {
		loop GuardedWorkflow {
			body Loop {
				loop GuardedWorkflow {
					body Sequence {
						subworkflows {
							Task {
								ID "AlertUser"
							},
							Task {
								ID "Vacuum"
							},
							Task {
								ID "LeaveRoom"
							}
						}
					}
					guard Atom {
						measureID "needsVacuuming"
					}
				}
			}
			guard Atom {
				measureID "not personPresent"
			}
		}
	}
	
}