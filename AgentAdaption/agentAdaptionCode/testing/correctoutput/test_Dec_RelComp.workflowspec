WorkflowStructure {
	workflowspec Decision {
		options {
			GuardedWorkflow {
				body Sequence {
					subworkflows {
						Decision {
				options {
					GuardedWorkflow {
						body Task {
							ID "Pause"
						}
						guard RelComp {
							op GREATER_THAN
							left Atom {
								measureID "unmappedBumperCount"
							}
							right Value {
								value 10	
							}
						}
					},
					GuardedWorkflow {
						body Task {
							ID "Skip"
						}
						guard Not {
							op "not"
							expr RelComp {
								op GREATER_THAN
								left Atom {
									measureID "unmappedBumperCount"
								}
								right Value {
									value 10
								}
							}
						}
					}
				}
			},
			Task {
				ID "Vacuum"
			}
					}
				}
				guard Atom {
					measureID "needsVacuuming and not personPresent"
				}
			},
			GuardedWorkflow {
				body Task {
					ID "Skip"
					}
					guard Atom {
						measureID "not needsVacuuming"
					}
				},
			GuardedWorkflow {
				body Task {
					ID "Dock"
					}
					guard Atom {
						measureID "personPresent"
				}
			}
		}
	}
}