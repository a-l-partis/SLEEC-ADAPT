WorkflowStructure {
	workflowspec Loop {
		loop GuardedWorkflow {
			body Sequence {
		subworkflows {
			Task {
				ID "Undock"
			},
			Task {
				ID "GoToRoom"
			},
			Task {
				ID "AlertUser"
			},
			Task {
				ID "Vacuum"
			},
			Task {
				ID "LeaveRoom"
			},
			Task {
				ID "beep"
			},
			Task {
				ID "Dock"
			}
		}	
	}
			guard Atom {
				measureID "needsVacuuming"
			}
		}
	}
}