WorkflowStructure {
	workflowspec Decision {
		options {
			GuardedWorkflow {
				body Sequence {
					subworkflows {
						Decision {
				options {
					GuardedWorkflow {
						body Task {
								ID "SetSilentCleaningMode"
							}
							guard Not {
								op "Not"
								expr Atom {
									measureID "daytime"
								}
							}
						},
						GuardedWorkflow {
								body Task {
									ID "Skip"
									}
								guard Not {
									op "Not"
									expr Not {
										op "Not"
										expr Atom {
											measureID "daytime"
										}
									}
								}
							}
					}
				},
						Task {
							ID "Vacuum"
						}
					}
				}
				guard Atom {
					measureID "needsVacuuming and not personPresent"
				}
			},
			GuardedWorkflow {
				body Task {
					ID "Skip"
					}
					guard Atom {
						measureID "not needsVacuuming"
					}
				},
			GuardedWorkflow {
				body Task {
					ID "Dock"
					}
					guard Atom {
						measureID "personPresent"
				}
			}
		}
	}
}