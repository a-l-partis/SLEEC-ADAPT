WorkflowStructure {
	workflowspec Decision {
		options {
			GuardedWorkflow {
				
				body Sequence {
					subworkflows {
						Task {
							ID "AlertUser"
						},
						Task {
							ID "Vacuum"
						},
						Task {
							ID "LeaveRoom"
							}
						}
					}
					guard Atom {
					measureID "needsVacuuming and not personPresent"
				}
			},
			GuardedWorkflow {
				body Task {
					ID "Skip"
					}
					guard Atom {
						measureID "not needsVacuuming"
					}
				},
			GuardedWorkflow {
				body Task {
					ID "Dock"
					}
					guard Atom {
						measureID "personPresent"
				}
			}
		}
	}
}