WorkflowStructure {
	workflowspec Decision {
		options {
			GuardedWorkflow {
				body Decision {
					options {
						GuardedWorkflow {
							body Sequence {
								subworkflows {
									Task {
										ID "AlertUser"
									},
									Task {
										ID "Vacuum"
									},
									Task {
										ID "LeaveRoom"
									}
								}
							}
							guard Atom {
								measureID "not personPresent"
							}
						}
					}
				}
				guard Atom {
					measureID "needsVacuuming"
				}
			}
		}
	}
}